{
  "name": "pokedex",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test:unit": "vitest"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "customExportConditions": [
        "node",
        "node-addons"
      ]
    },
    "moduleNameMapper": {
      "^@vue/test-utils": "<rootDir>/node_modules/@vue/test-utils/dist/vue-test-utils.cjs.js"
    }
  },
  "dependencies": {
    "axios": "^1.7.7",
    "pinia": "^2.2.2",
    "vue": "^3.4.37",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/test-utils": "^2.4.6",
    "jest": "^29.7.0",
    "jsdom": "^25.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vitest": "^2.1.1",
    "vue-tsc": "^2.0.29"
  }
}
